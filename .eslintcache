 {\n    const { product, fetching, error, addProductToCart } = this.props;\n    return (\n      <div className=\"container mt-5\">\n        {fetching ? (\n          <p className=\"text-center\">Fetching...</p>\n        ) : error ? (\n          <p className=\"text-center\">Something went wrong</p>\n        ) : product ? (\n          <div className=\"d-flex flex-wrap justify-content-center\">\n            <div className=\"product-image mr-lg-5 mr-0\">\n              <img\n                className=\"img-fluid h-100\"\n                src={product.image}\n                alt={product.title}\n              />\n            </div>\n\n            <div className=\"product-body d-flex flex-column justify-content-between mt-5\">\n              <div className=\"d-flex flex-column\">\n                <h2>{product.title}</h2>\n                <span className=\"mb-3\">{product.category}</span>\n                <h3 className=\"product-price\">${product.price}</h3>\n              </div>\n              <div className=\"d-flex flex-column flex-grow-1 justify-content-around\">\n                <div className=\"product-quantity-container d-flex align-items-baseline\">\n                  <h5 className=\"mr-3\">Quantity: </h5>\n                  <button\n                    onClick={() => this.incrementQuantity()}\n                    className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                  >\n                    +\n                  </button>\n                  <span className=\"product-quantity mr-3\">\n                    {this.state.quantity}\n                  </span>\n                  <button\n                    onClick={() => this.decrementQuantity()}\n                    className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                  >\n                    -\n                  </button>\n                </div>\n                <button\n                  className=\"btn btn-primary\"\n                  onClick={() => {\n                    if (product.quantity !== this.state.quantity) {\n                      product.quantity = this.state.quantity;\n                      addProductToCart(product);\n                    }\n                  }}\n                >\n                  Add To Cart\n                </button>\n              </div>\n            </div>\n          </div>\n        ) : (\n          <p className=\"text-center\">No Products Available</p>\n        )}\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = ({ productsState, orderState }, props) => ({\n  product: productsState.product,\n  quantity: getProductQuantity(orderState, props.match.params.id),\n  fetching: productsState.fetching,\n  error: productsState.error,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  getProduct: (id) => dispatch(createSingleProductFetchAction(id)),\n  addProductToCart: (product) =>\n    dispatch(createAddProductToOrderAction(product)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ProductDetails);\n","/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/index.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/actions/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/types/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/selectors/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/sagas/index.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/selectors/order.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/components/Header/Header.jsx",["114"],"import React from \"react\";\nimport Dropdown from \"react-bootstrap/Dropdown\";\nimport avatar from \"../../assets/images/avatar.png\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faShoppingCart, faTrash } from \"@fortawesome/free-solid-svg-icons\";\nimport \"./Header.scss\";\nimport { connect } from \"react-redux\";\nimport {\n  createIncrementQuantityAction,\n  createDecrementQuantityAction,\n  createDeleteProductFromOrderAction,\n} from \"../../store/actions/order\";\nimport { Link } from \"react-router-dom\";\nimport { useState } from \"react\";\n\nconst Header = ({\n  selectedProducts,\n  incrementQuantity,\n  decrementQuantity,\n  deleteProduct,\n}) => {\n  const dropdownRef = React.createRef();\n\n  return (\n    <nav className=\"navbar navbar-light bg-light justify-content-between\">\n      <Link className=\"navbar-brand\" to=\"/products\">\n        Products List\n      </Link>\n\n      <ul className=\"navbar-nav flex-row\">\n        <Dropdown className=\"mr-3\">\n          <Dropdown.Toggle variant=\"light\">\n            <FontAwesomeIcon icon={faShoppingCart} />\n          </Dropdown.Toggle>\n\n          <Dropdown.Menu className=\"selected-products-menu\" ref={dropdownRef}>\n            {selectedProducts && selectedProducts.length > 0 ? (\n              <React.Fragment>\n                <Dropdown.Header>Your Selected Products</Dropdown.Header>\n                {selectedProducts.map((product) => (\n                  <div key={product.id} className=\"px-4 py-3 product-item\">\n                    <div className=\"d-flex flex-wrap justify-content-between\">\n                      <span className=\"mr-3 product-title\">\n                        {product.title} ({product.quantity})\n                      </span>\n                      <div className=\"buttons-container d-flex justify-content-lg-end justify-content-center flex-grow-1\">\n                        <button\n                          onClick={(e) => incrementQuantity(product.id)}\n                          className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                        >\n                          +\n                        </button>\n                        <button\n                          onClick={(e) => decrementQuantity(product.id)}\n                          className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                        >\n                          -\n                        </button>\n                        <button\n                          onClick={(e) => deleteProduct(product.id)}\n                          className=\"quantity-btn btn btn-danger btn-sm mr-3\"\n                        >\n                          <FontAwesomeIcon icon={faTrash} />\n                        </button>\n                      </div>\n                    </div>\n                  </div>\n                ))}\n                <div className=\"px-4 py-3\">\n                  <Link\n                    to=\"/order\"\n                    className=\"btn btn-dark btn-sm w-100 review-order-btn\"\n                    onClick={() => dropdownRef.current.classList.toggle(\"show\")}\n                  >\n                    Review Order\n                  </Link>\n                </div>\n              </React.Fragment>\n            ) : (\n              <Dropdown.Header className=\"text-center\">\n                No Products in Cart\n              </Dropdown.Header>\n            )}\n          </Dropdown.Menu>\n        </Dropdown>\n\n        <Dropdown>\n          <Dropdown.Toggle variant=\"light\">\n            <img className=\"img-fluid\" src={avatar} alt=\"Profile\" />\n          </Dropdown.Toggle>\n\n          <Dropdown.Menu>\n            <Dropdown.Item>View/ Edit Profile</Dropdown.Item>\n            <Dropdown.Item className=\"signout\">Sign Out</Dropdown.Item>\n          </Dropdown.Menu>\n        </Dropdown>\n      </ul>\n    </nav>\n  );\n};\n\nconst mapDispatchToProps = (dispatch) => ({\n  incrementQuantity: (id) => dispatch(createIncrementQuantityAction(id)),\n  decrementQuantity: (id) => dispatch(createDecrementQuantityAction(id)),\n  deleteProduct: (id) => dispatch(createDeleteProductFromOrderAction(id)),\n});\n\nconst mapStateToProps = ({ orderSt