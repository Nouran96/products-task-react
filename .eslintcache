ts.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/components/ProductCard/ProductCard.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/types/order.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/actions/order.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/reducers/order.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/routes/Routes.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/containers/ProductDetails/ProductDetails.jsx",["101","102"],"import React, { Component } from \"react\";\nimport { connect } from \"react-redux\";\nimport { createSingleProductFetchAction } from \"../../store/actions/products\";\nimport { createAddProductToOrderAction } from \"../../store/actions/order\";\nimport { getProductQuantity } from \"../../store/selectors/order\";\nimport \"./ProductDetails.scss\";\n\nclass ProductDetails extends Component {\n  state = {\n    quantity: 0,\n  };\n\n  componentDidMount() {\n    this.props.getProduct(this.props.match.params.id);\n    this.setState({ quantity: this.props.quantity });\n  }\n\n  incrementQuantity = () => {\n    this.setState({ quantity: ++this.state.quantity });\n  };\n\n  decrementQuantity = () => {\n    this.setState({ quantity: --this.state.quantity });\n  };\n\n  render() {\n    const { product, fetching, error, addProductToCart } = this.props;\n    return (\n      <div className=\"container mt-5\">\n        {fetching ? (\n          <p className=\"text-center\">Fetching...</p>\n        ) : error ? (\n          <p className=\"text-center\">Something went wrong</p>\n        ) : product ? (\n          <div className=\"d-flex flex-wrap justify-content-center\">\n            <div className=\"product-image mr-lg-5 mr-0\">\n              <img\n                className=\"img-fluid h-100\"\n                src={product.image}\n                alt={product.title}\n              />\n            </div>\n\n            <div className=\"product-body d-flex flex-column justify-content-between mt-5\">\n              <div className=\"d-flex flex-column\">\n                <h2>{product.title}</h2>\n                <span className=\"mb-3\">{product.category}</span>\n                <h3 className=\"product-price\">${product.price}</h3>\n              </div>\n              <div className=\"d-flex flex-column flex-grow-1 justify-content-around\">\n                <div className=\"product-quantity-container d-flex align-items-baseline\">\n                  <h5 className=\"mr-3\">Quantity: </h5>\n                  <button\n                    onClick={() => this.incrementQuantity()}\n                    className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                  >\n                    +\n                  </button>\n                  <span className=\"product-quantity mr-3\">\n                    {this.state.quantity}\n                  </span>\n                  <button\n                    onClick={() => this.decrementQuantity()}\n                    className=\"quantity-btn btn btn-dark btn-sm mr-3\"\n                  >\n                    -\n                  </button>\n                </div>\n                <button\n                  className=\"btn btn-primary\"\n                  onClick={() => {\n                    if (product.quantity !== this.state.quantity) {\n                      product.quantity = this.state.quantity;\n                      addProductToCart(product);\n                    }\n                  }}\n                >\n                  Add To Cart\n                </button>\n              </div>\n            </div>\n          </div>\n        ) : (\n          <p className=\"text-center\">No Products Available</p>\n        )}\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = ({ productsState, orderState }, props) => ({\n  product: productsState.product,\n  quantity: getProductQuantity(orderState, props.match.params.id),\n  fetching: productsState.fetching,\n  error: productsState.error,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  getProduct: (id) => dispatch(createSingleProductFetchAction(id)),\n  addProductToCart: (product) =>\n    dispatch(createAddProductToOrderAction(product)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ProductDetails);\n","/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/index.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/actions/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/types/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/selectors/products.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/sagas/index.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/store/selectors/order.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/components/Header/Header.jsx",["103"],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/containers/OrderReview/OrderReview.jsx",[],"/home/nouran/Documents/Ibtikar/Training_Article/products_task/products_task/src/containers/OrderForm/OrderForm.jsx",[],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"104","replacedBy":"108"},{"ruleId":"106","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":19,"column":33,"nodeType":"112","endLine":19,"endColumn":43},{"ruleId":"110","severity":1,"message":"111","line":23,"column":33,"nodeType":"112","endLine":23,"endColumn":43},{"ruleId":"113","severity":1,"message":"114","line":14,"column":10,"nodeType":"115","messageId":"116","endLine":14,"endColumn":18},"no-nat